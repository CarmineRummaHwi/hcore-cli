{
    "name": "hcore/cli",
    "version": "v0.2.4",
    "description": "HCore Command Line Tools",
    "keywords": ["hcore"],
    "license": "MIT",
    "authors": [
        {
            "name": "Carmine Rumma",
            "email": "carmine.rumma@healthwareinternational.com",
            "homepage": "https://www.healthwaregroup.com/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "vimeo/psalm": "^3.7",
